@(reviewList: List[RecetaReview])
<?xml version="1.0" encoding="UTF-8" ?>
<reviews>
    @if(!reviewList.isEmpty()) {
        <autor>@usuarioinfo(reviewList(0).autor)</autor>
        @for(rvw <- reviewList) {
            <review>
                <receta>@recetainfo(rvw.receta)</receta>
                <nota>@rvw.nota</nota>
            </review>
        }
    }
</reviews>
w